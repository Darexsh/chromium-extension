{
    "manifest_version": 3,
    "name": "__MSG_extensionName__",
    "version": "2.0.1",
    "description": "__MSG_extensionDescription__",
    "author": "Daniel Sichler",
    "default_locale": "en",
    "icons": {
      "128": "icons/One_Piece/icon128.png",
      "48": "icons/One_Piece/icon48.png",
      "16": "icons/One_Piece/icon16.png"
    },
    "chrome_url_overrides": {
      "newtab": "tab.html"
    },
    "permissions": [
      "storage",
      "unlimitedStorage"
    ],
    "host_permissions": [
      "https://get.api-feiertage.de/*"
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'; frame-src https://api.wetteronline.de https://www.metercustom.net"
    }
}
