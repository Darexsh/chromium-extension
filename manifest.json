{
    "manifest_version": 3,
    "name": "__MSG_extensionName__",
    "version": "6.0",
    "description": "__MSG_extensionDescription__",
    "author": "Daniel Sichler",
    "default_locale": "en",
    "icons": {
      "128": "icons/One_Piece/icon128.png",
      "48": "icons/One_Piece/icon48.png",
